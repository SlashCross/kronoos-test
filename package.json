{
  "name": "kronoos-test",
  "version": "1.0.0",
  "description": "## Manipulação de Dados de CSV e Conversão para Array",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./bin/server.js",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "migrate": "npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kronoos-dev/kronoos-test.git"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "keywords": [],
  "author": "Gabriel Montibeller",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kronoos-dev/kronoos-test/issues"
  },
  "homepage": "https://github.com/kronoos-dev/kronoos-test#readme",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "csv": "^6.3.3",
    "csv-parse": "^5.5.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.15.4",
    "express-validator": "^7.0.1",
    "intl": "^1.2.5",
    "mysql2": "^3.6.1",
    "sequelize": "^6.33.0",
    "sequelize-cli": "^6.6.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  }
}
